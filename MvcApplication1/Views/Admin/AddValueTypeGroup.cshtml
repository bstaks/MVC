@model MvcApplication1.DataModels.ValueTypeGroupModels

@{
    ViewBag.Title = "AddValueTypeGroup";
}

<h2>AddValueTypeGroup</h2>
<div class="row">
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

   
        <div class="form-group">
        <div class="col-md-10">
            @Html.LabelFor(model => model.ValueTypeGroupName)
        </div>
        <div class="clearfix"></div>
        <div class="col-md-4">
            @Html.TextBoxFor(model => model.ValueTypeGroupName,new {@class ="form-control"})
            @Html.ValidationMessageFor(model => model.ValueTypeGroupName)
        </div>
        </div>
        <div class="col-md-10">
            <input type="submit" class="btn btn-default" value="Save" />
        </div>

}

</div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
